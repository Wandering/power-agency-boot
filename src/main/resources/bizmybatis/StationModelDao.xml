<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.power.dao.StationModelDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.power.entity.StationModelEntity" id="stationModelMap">
        <result property="id" column="id"/>
        <result property="model" column="model"/>
        <result property="slotNo" column="slot_no"/>
        <result property="channel" column="channel"/>
        <result property="inputVoltage" column="input_voltage"/>
        <result property="maxInputCurrent" column="max_input_current"/>
        <result property="maxInputPower" column="max_input_power"/>
        <result property="staticInputPower" column="static_input_power"/>
        <result property="authentication" column="authentication"/>
        <result property="size" column="size"/>
        <result property="isnfc" column="isNFC"/>
        <result property="deliveryModel" column="delivery_model"/>
        <result property="manufacturer" column="manufacturer"/>
        <result property="createDt" column="create_dt"/>
        <result property="updateDt" column="update_dt"/>
        <result property="createBy" column="create_by"/>
        <result property="updateBy" column="update_by"/>
    </resultMap>

	<select id="queryObject" resultType="com.power.entity.StationModelEntity">
		select * from station_model where id = #{value}
	</select>

	<select id="queryList" resultType="com.power.entity.StationModelEntity">
		select * from station_model
        <where>
			                <!--  -->
                <if test="id!=null and id.trim()!=null">
                    AND id = #{id}
                </if>
			                <!-- 充电桩型号 -->
                <if test="model!=null and model.trim()!=null">
                    AND model = #{model}
                </if>
			                <!-- 卡槽数量 -->
                <if test="slotNo!=null and slotNo.trim()!=null">
                    AND slot_no = #{slotNo}
                </if>
			                <!-- 通讯方式 -->
                <if test="channel!=null and channel.trim()!=null">
                    AND channel = #{channel}
                </if>
			                <!-- 输入电压 -->
                <if test="inputVoltage!=null and inputVoltage.trim()!=null">
                    AND input_voltage = #{inputVoltage}
                </if>
			                <!-- 最大输入电流 -->
                <if test="maxInputCurrent!=null and maxInputCurrent.trim()!=null">
                    AND max_input_current = #{maxInputCurrent}
                </if>
			                <!-- 最大输入功耗 -->
                <if test="maxInputPower!=null and maxInputPower.trim()!=null">
                    AND max_input_power = #{maxInputPower}
                </if>
			                <!-- 静态输入功耗 -->
                <if test="staticInputPower!=null and staticInputPower.trim()!=null">
                    AND static_input_power = #{staticInputPower}
                </if>
			                <!-- 认证 -->
                <if test="authentication!=null and authentication.trim()!=null">
                    AND authentication = #{authentication}
                </if>
			                <!-- 尺寸 -->
                <if test="size!=null and size.trim()!=null">
                    AND size = #{size}
                </if>
			                <!-- NFC -->
                <if test="isnfc!=null and isnfc.trim()!=null">
                    AND isNFC = #{isnfc}
                </if>
			                <!-- 出仓方式 -->
                <if test="deliveryModel!=null and deliveryModel.trim()!=null">
                    AND delivery_model = #{deliveryModel}
                </if>
			                <!-- 生产厂家 -->
                <if test="manufacturer!=null and manufacturer.trim()!=null">
                    AND manufacturer = #{manufacturer}
                </if>
			                <!--  -->
                <if test="createDt!=null and createDt.trim()!=null">
                    AND create_dt = #{createDt}
                </if>
			                <!--  -->
                <if test="updateDt!=null and updateDt.trim()!=null">
                    AND update_dt = #{updateDt}
                </if>
			                <!--  -->
                <if test="createBy!=null and createBy.trim()!=null">
                    AND create_by = #{createBy}
                </if>
			                <!--  -->
                <if test="updateBy!=null and updateBy.trim()!=null">
                    AND update_by = #{updateBy}
                </if>
			        </where>
        <choose>
            <when test="sidx != null and sidx.trim() != ''">
                order by ${sidx} ${order}
            </when>
			<otherwise>
                order by id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
